<script>
	/**BEGIN JAVASCRIPT **/
if (subscription_on==true) {

    window.currency_symbol='$'
    window.currency_suffix=''
    
    discount_percentage_metafield_value = 10 
    has_discount_171431270 = false
    if(discount_percentage_metafield_value > 0) {
		has_discount_171431270 = true 
	}
	
	/* for changing background color when a selection is clicked */
	rjQuery('body').on('click','.autodeliver.171431270',function() {
		rjQuery('.radio_option.171431270').removeClass('active')
		rjQuery(this).parent().parent().addClass('active')

		if( rjQuery(this).val()=='autodeliver' ) {
			rjQuery('#recurring_purchase_171431270').addClass('active')
			rjQuery('#single_purchase_171431270').removeClass('active')
		} else if( rjQuery(this).val()=='onetime' ) {
			rjQuery('#recurring_purchase_171431270').removeClass('active')
			rjQuery('#single_purchase_171431270').addClass('active')
		}

		if(has_discount_171431270 == true) {
			updatePrice_171431270("UPDATE - Change radio");    
		}
	})
	
	 //HANDLE THE ADDING TO CART
	rjQuery( document ).ready(function() {
		// CREATE DUPLICATE SELECT WITH OPTIONS
      	var myDiv = rjQuery('form[data-productid="171431270"]');
		var discount_prices = discount_variants_array_for_select_171431270; //discount price options
		//Create and append select list
		var discount_select_menu_html = rjQuery("<select id='discount-select-171431270'></select>"); 
		myDiv.append(discount_select_menu_html);
		for (var i = 0; i < discount_prices.length; i++) {
			var option = rjQuery('<option></option>').val(discount_prices[i]).html(discount_prices[i])
			discount_select_menu_html.append(option);
		}

        // ADD IDENTIFIERS TO ORIGINAL AND DUPLICATE SELECT        
        var discount_select_171431270 = rjQuery("#discount-select-171431270")
        discount_select_171431270.hide();  
        
        var original_select_171431270 = rjQuery('[name="id"][data-productid="171431270"]')
		original_select_171431270.addClass('original_select_171431270')
        // Set value of discount select on page load
        var original_select_menu_value = original_select_171431270.val();
		var discount_value_of_originally_selected_menu_value = variant_id_to_discount_id_map_171431270[original_select_menu_value]['discount_variant_id'];
		discount_select_171431270.val(discount_value_of_originally_selected_menu_value);	

        // using a timeout to wait for shopify to load select elements via javascript
        setTimeout(function(){
        	
        	// INITIALIZE LISTENER FOR WHEN VARIANT DROPDOWN IS CHANGED 
			rjQuery('form[data-productid="171431270"] .single-option-selector').change(function(){
				
				setTimeout(function(){
					var original_product_value = original_select_171431270.val();	
					discount_select_171431270.val(variant_id_to_discount_id_map_171431270[original_product_value]['discount_variant_id']);	
					
					updatePrice_171431270();
				}, 400);
				
			})

            // LISTENER FOR WHEN RADIO OPTION FOR ONE-TIME/RECURRING IS CHECKED                
            rjQuery(".autodeliver.171431270").change(function(){				
				/*IF AUTODELIVER IS SELECTED, WE SET OUR SELECT to name=id and take that attribute off the original select*/	
				if(rjQuery(this).val() == 'autodeliver'){
					// set duplicate select to be submitted 
					original_select_171431270.attr('name','')
					discount_select_171431270.attr('name','id')
					//set shipping interval + subscription id attributes to be submitted when form is submitted
					rjQuery("#shipping_interval_frequency_171431270").attr('name','properties[shipping_interval_frequency]');
					rjQuery("#subscription_id_171431270").attr('name','properties[subscription_id]');
					rjQuery("#shipping_interval_unit_type_171431270").attr('name','properties[shipping_interval_unit_type]');

				}
              else if (rjQuery(this).val() == 'onetime') {
                  // set original select to be submitted
                  original_select_171431270.attr('name','id')
                  discount_select_171431270.attr('name','')
                  
                  // remove shipping interval + subscription id from form submission since we're not subscription
                  rjQuery("#shipping_interval_frequency_171431270").attr('name','');
                  rjQuery("#subscription_id_171431270").attr('name','');
				  rjQuery("#shipping_interval_unit_type_171431270").attr('name','');
              }
            });
        }, 300); // end setTimeout / initialization of select menu listeners

        // IF SUBSCRIPTION ONLY, preset the subscription inputs
        
      });
 
 
	/*** THIS PERFORMS PRICE UPDATE of primary price text above options***/
	function updatePrice_171431270(debugging_text) {
		var price_selectors = '.current_price, #productPrice, .product-price, .price, #price-preview, .actual-price'
		
		//check for quickbuy and adjust selectors if needed
		if ( rjQuery(price_selectors).size() > 1 ) {
			var has_quickbuy = true
			//the quickbuy price selectors
			price_selectors = '.current_price.171431270, #productPrice.171431270, .product-price.171431270, .price.171431270, #price-preview.171431270'
		}
		
		rjQuery(price_selectors).html(''); //blank out the current price
		var variant_id = rjQuery('.original_select_171431270').val()      
		var quantity = rjQuery('form[data-productid="171431270"] [name="quantity"]').val()
	  
		if(typeof quantity === 'undefined'){
			//some sites don't define quantity in the form so we need this as a backup
			quantity = 1;
		}
		
		variant_price_171431270 = (variant_id_to_price_map_171431270[variant_id] / 100) 
		
		//if recurring is on, set 
		if( rjQuery('#autodeliver_on_radio_171431270').prop('checked')==true && variant_id_to_discount_id_map_171431270[variant_id]['discount_variant_id'] !='') {
			variant_price_171431270 = variant_price_to_discount_price_map_171431270[variant_id]['discount_variant_price']
		} 

		variant_price_171431270 = parseFloat(variant_price_171431270).toFixed(2)
		var displayed_price = parseFloat(quantity * variant_price_171431270).toFixed(2)
		var price_string = currency_symbol+' '+displayed_price + currency_suffix
	  
		if(window.first_time_171431270){
			rjQuery(price_selectors).html(price_string)
		}
		window.first_time_171431270 = true;
		updateInlinePrice_171431270()
	}

	// This function updates the inline prices (next to radio buttons) 
	function updateInlinePrice_171431270() {
		var variant_id = rjQuery('.original_select_171431270').val();
		var price_one_time = (variant_id_to_price_map_171431270[variant_id] / 100);
		price_one_time = parseFloat(price_one_time).toFixed(2);
        
        var price_recurring = variant_price_to_discount_price_map_171431270[variant_id]['discount_variant_price']
        price_recurring = parseFloat(price_recurring).toFixed(2);

		rjQuery('#one-time-price_171431270').text(" "+currency_symbol+ price_one_time + currency_suffix);
		rjQuery('#recurring-time-price_171431270').text(" "+currency_symbol + price_recurring + currency_suffix);
	}
	
	/** PRICE RELATED DOCUMENT READY **/
	rjQuery(document).ready(function(){
		
		/** make sure correct option has grey border **/
		window.lock_171431270 = false;
		window.first_time_171431270 = true;
		if(rjQuery('#autodeliver_on_radio_171431270').prop('checked')==true ) {
			rjQuery('#recurring_purchase_171431270').addClass('active');
		} else {
			rjQuery('#single_purchase_171431270').addClass('active');
		}
		if(has_discount_171431270 == true) {
			setTimeout(function(){ updatePrice_171431270("") }, 200);
		}
		
		
		//if something modifies the price, we notice it and then change it back. but we use a timeout function to avoid an infinite loop
		rjQuery('.current_price, #productPrice, .product-price, .price, #price-preview').bind("DOMSubtreeModified",function(){      
			if( window.lock_171431270 == false ) {
				window.lock_171431270 = true;
				if(has_discount_171431270 == true) {
					updatePrice_171431270("UPDATE - DOMSubtreeModified");
				}
				setTimeout(function(){window.lock_171431270=false}, 200);
			}
		});
		
		//handle change in quantity
		rjQuery('#quantity').on("change",function(){
			if(has_discount_171431270 == true) {
				updatePrice_171431270("UPDATE - Qty change on");
			}
		});
		
		//run the price updater once page loads to reset it to what we need it to be				
		updateInlinePrice_171431270();		
				
	})//end document ready

		

	/** set variant mapping for the case in which there is an automatically created duplicate product **/
	var variant_id_to_discount_id_map_171431270 = {};  
	var variant_price_to_discount_price_map_171431270 ={} 
	var discount_variants_array_for_select_171431270=[]
	
	
		var discount_variant_id = '5802299073'
		variant_id_to_discount_id_map_171431270['393428714']='' 
		
		if(discount_variant_id) {
			variant_id_to_discount_id_map_171431270['393428714'] = {"discount_variant_id":discount_variant_id};
			discount_variants_array_for_select_171431270.push(discount_variant_id)
		}
		
		var discount_variant_price = '21.59'
		variant_price_to_discount_price_map_171431270['393428714']=''
		
		if(discount_variant_price) {
			variant_price_to_discount_price_map_171431270['393428714'] = { "discount_variant_price": discount_variant_price } 
		}
	
	
	//map original prices for updated price calculations
	variant_id_to_price_map_171431270={}
	
		variant_id_to_price_map_171431270['393428714'] = "2399"
	

}//end if (subscription_on==true) section
	
</script> 
 
<!-- subscription code ends -->
     
    
