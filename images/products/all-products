        //case for subscription only with shipping interval frequency dropdown

  var multiple_frequency_options_171431270=false
  
    var multiple_frequency_options_171431270=true
  
  if (subscription_on==true && subscription_only_171431270==true && multiple_frequency_options_171431270) {
      rjQuery('#recurring_choice_171431270').show()
      rechargedebug['showinghtml']='true'
      rjQuery('#single_purchase_171431270').hide()
      rjQuery('#recurring_purchase_171431270 label').eq(0).hide()
        
